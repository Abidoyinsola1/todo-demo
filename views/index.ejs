<%- include('partials/header.ejs') %>


  <body>
    <div class="row justify-content-md-center list">
      <div class="col-md-5">
        <h4 class="navigation"><%= date %></h4>
        <% addTask.map((item) => {%>
        <div class="itemList">
          <input type="checkbox" />
          <p class="items"><%= item %></p>
        </div>
        <% })%>

        <form action="/" method="post">
          <input
            type="text"
            name="newTask"
            placeholder="Add New Task"
            autocomplete="off"
          />
          <button type="submit"><i class="fa fa-plus"></i></button>
        </form>
      </div>
    </div>

    <%- include('partials/footer.ejs') %>

